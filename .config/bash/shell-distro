#!/usr/bin/env sh

# set -e

[[ $(command -v dnf) ]] && source "$XDG_CONFIG_HOME/bash/distro/shell-fedora"
[[ $(command -v apt) ]] && source "$XDG_CONFIG_HOME/bash/distro/shell-debian"
[[ $(command -v snap) ]] && source "$XDG_CONFIG_HOME/bash/distro/shell-ubuntu"
[[ $(command -v nix-shell) ]] && source "$XDG_CONFIG_HOME/bash/distro/shell-nixos"
