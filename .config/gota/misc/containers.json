{
    "task": "containers",
    "deps": [
        "podman"
    ],
    "command": "podman pull",
    "jobs": {
        "languages": [
            "docker.io/library/composer:lts",
            "docker.io/library/node:18-slim",
            "docker.io/library/php:8",
            "docker.io/library/python:3-slim",
            "docker.io/library/ruby:3-slim"
        ],
        "database": [
            "docker.io/library/postgres:15",
            "docker.io/library/mariadb:10.7"
        ],
        "utils": [
            "docker.io/library/nicolaka/netshoot"
        ],
        "distributions": [
            "docker.io/library/debian:stable-slim",
            "docker.io/library/debian:unstable-slim",
            "docker.io/library/ubuntu:lunar",
            "docker.io/library/busybox:glibc"
        ]
    }
}
