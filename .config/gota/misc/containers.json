{
    "task": "containers",
    "deps": [
        "podman"
    ],
    "command": "podman pull",
    "jobs": {
        "langs": [
            "docker.io/library/python:3-slim",
            "docker.io/library/ruby:3-slim",
            "docker.io/library/composer:lts",
            "docker.io/library/php:8"
        ],
        "db": [
            "docker.io/library/postgres:15"
        ],
        "distro": [
            "docker.io/library/debian:stable-slim",
            "docker.io/library/debian:unstable-slim"
        ]
    }
}
