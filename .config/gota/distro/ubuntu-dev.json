{
  "manager": "ubuntu-dev",
  "command": "sudo apt-get install -y",
  "packages": {
    "postgres": [
      "postgresql",
      "postgresql-client",
      "postgresql-contrib",
      "libpq-dev"
    ],
    "langs": [
      "nodejs",
      "npm"
    ],
    "python_tools": [
      "python3-pip",
      "python3-venv"
    ],
    "tools": [
      "jq",
      "git-email",
      "subversion",
      "mercurial",
      "libpq-dev",
      "editorconfig",
      "direnv",
      "fd-find",
      "shfmt",
      "meson",
      "cmake",
      "grip"
    ],
    "editors": [
      "vim",
      "tidy",
      "ripgrep"
    ],
    "alacritty": [
      "pkg-config",
      "libfreetype6-dev",
      "libfontconfig1-dev",
      "libxcb-xfixes0-dev",
      "libxkbcommon-dev"
    ]
  }
}
