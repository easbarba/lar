{
    "manager": "debian-dev",
    "command": "sudo apt-get install -y",
    "deps": [
        "apt"
    ],
    "packages": {
        "langs": [
            "python3-pip",
            "nodejs",
            "npm"
        ],
        "tools": [
            "jq",
            "git-email",
            "subversion",
            "mercurial",
            "libpq-dev",
            "editorconfig",
            "direnv",
            "fd-find",
            "shfmt",
            "meson",
            "cmake",
            "grip"
        ],
        "editors": [
            "vim",
            "tidy",
            "ripgrep"
        ],
        "alacritty": [
            "pkg-config",
            "libfreetype6-dev",
            "libfontconfig1-dev",
            "libxcb-xfixes0-dev",
            "libxkbcommon-dev"
        ]
    }
}
