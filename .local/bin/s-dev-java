#!/usr/bin/env bash

# Debug Options
set -euo pipefail

DEPS=(curl bash)
s-checkdeps "${DEPS[@]}"

COMMAND='sdk install'

[[ ! -f $HOME/.sdkman/bin/sdkman-init.sh ]] && curl -s "https://get.sdkman.io" | bash

source "$HOME/.sdkman/bin/sdkman-init.sh"

packages=(gradle maven)
s-dolist "$COMMAND" "${packages[@]}"
